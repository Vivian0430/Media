</body>
<script>
    var phone=window.sessionStorage.getItem("phone");
    // console.log(phone);
    if(phone){
        $("#login").text("欢迎"+phone);
        $("#register").css("display","none");
    }

    $(".thumbnail").click(function(){
        console.log("kakak")
        var imgSrc=$(this).data("picture");
        console.log(imgSrc);
       $("#imgShow").attr('src',imgSrc); 
       $(".picture_play").css("display","block");

    })

    $(".picture_play").click(function(){
        $(".picture_play").css("display","none");
    })
</script>
</html>